<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="shortcut icon" href="https://www.freeiconspng.com/uploads/book-icon--icon-search-engine-6.png" type="image/x-icon">
        <link rel="stylesheet" href="/styles/reset.css">
        <link rel="stylesheet" href="/styles/base.css">
        <link rel="stylesheet" href="/styles/layout.css">
        <link rel="stylesheet" href="/styles/modules.css">
        <title>Show</title>
    </head>
<body>
    <nav>
        <h1>Best Books!</h1>
        <img src="https://external-content.duckduckgo.com/iu/?u=https%3A%2F%2Fupload.wikimedia.org%2Fwikipedia%2Fcommons%2Fthumb%2Fb%2Fb2%2FHamburger_icon.svg%2F1200px-Hamburger_icon.svg.png&f=1&nofb=1" width="40px">
    </nav>
    <header>
        <h2>Search results</h2>
    </header>
    <main>
        <ul>  
            <% bookInfo.forEach(obj=>{ %>
                <div>
                    <% Object.values(obj).forEach(item=>{ %>
                        <% if(item === Object.values(obj)[1]){%>
                            <img src=<%=item%> >
                            <%}else{%>
                                <li><%= item %></li>
                            <%}%>
                    <% }) %>
                </div>
            <%});%>
        </ul>
    </main>
</body>
</html>